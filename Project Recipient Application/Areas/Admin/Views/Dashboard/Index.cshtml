
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayoutPage.cshtml";
}




<div class="col-lg-4 col-md-6">
    <div class="card-box">
        <div class="dropdown pull-right">
        </div>
        <h4 class="header-title m-t-0 m-b-30">پروژه های دانشجویان IT</h4>
        <div class="widget-box-2">
            <div class="widget-detail-2">
                <span class="badge badge-success  pull-left m-t-20">@(((string[])ViewData["Cu"])[3])% <i class="zmdi zmdi-trending-up"></i> </span>
                <h2 class="m-b-0">  @(((string[])ViewData["Cu"])[2])</h2>
                <p class="text-muted m-b-25">پروژه های دانشجویان IT</p>
            </div>

        </div>
    </div>
</div><!-- end col -->




<div class="col-lg-4 col-md-6">
    <div class="card-box">
        <div class="dropdown pull-right">
        </div>
        <h4 class="header-title m-t-0 m-b-30">پروژه های دانشجویان نرم افزار</h4>
        <div class="widget-box-2">
            <div class="widget-detail-2">
                <span class="badge badge-pink pull-left m-t-20">@(((string[])ViewData["Cu"])[1])% <i class="zmdi zmdi-trending-up"></i> </span>
                <h2 class="m-b-0">  @(((string[])ViewData["Cu"])[0]) </h2>
                <p class="text-muted m-b-25">پروژه های دانشجویان نرم افزار</p>
            </div>

        </div>
    </div>
</div><!-- end col -->
